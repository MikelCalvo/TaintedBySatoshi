{
  "name": "tainted-by-satoshi",
  "version": "1.0.0",
  "description": "Check if Bitcoin addresses are connected to Satoshi Nakamoto",
  "private": true,
  "scripts": {
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:backend": "pm2 start ecosystem.config.js --only TaintedBySatoshi_backend",
    "pm2:start:frontend": "pm2 start ecosystem.config.js --only TaintedBySatoshi_frontend",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:stop:backend": "pm2 stop TaintedBySatoshi_backend",
    "pm2:stop:frontend": "pm2 stop TaintedBySatoshi_frontend",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:restart:backend": "pm2 restart TaintedBySatoshi_backend",
    "pm2:restart:frontend": "pm2 restart TaintedBySatoshi_frontend",
    "pm2:reload:frontend": "cd frontend && npm run build && cd .. && pm2 restart TaintedBySatoshi_frontend",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:logs:backend": "pm2 logs TaintedBySatoshi_backend",
    "pm2:logs:frontend": "pm2 logs TaintedBySatoshi_frontend",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "deploy:frontend": "cd frontend && npm run build && cd .. && pm2 restart TaintedBySatoshi_frontend",
    "deploy:backend": "pm2 restart TaintedBySatoshi_backend",
    "deploy:all": "cd frontend && npm run build && cd .. && pm2 restart ecosystem.config.js",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "keywords": [
    "bitcoin",
    "satoshi",
    "blockchain",
    "taint-analysis"
  ],
  "author": "",
  "license": "ISC"
}
